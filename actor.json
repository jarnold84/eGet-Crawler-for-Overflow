{
  "name": "eget-crawler-overflow",
  "version": "0.1",
  "dockerfile": "Dockerfile",
  "startCommand": "python run.py",
  "defaultRunOptions": {
    "memoryMbytes": 2048,
    "timeoutSecs": 3600,
    "build": "latest"
  },
  "input": "json",
  "inputSchema": "input_schema.json",
  "mounts": [
    {
      "source": "input",
      "destination": "/apify/input.json"
    }
  ]
}
